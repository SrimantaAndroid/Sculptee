<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout
        xmlns:android="http://schemas.android.com/apk/res/android"

        android:id="@+id/drawer_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <include layout="@layout/header_layout">

    </include>
    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:layout_gravity="start">
        <ScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
    <LinearLayout
            android:id="@+id/drawer"
            android:layout_width="@dimen/_250sdp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@color/white">
        <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_50sdp"
                android:background="@color/colorPrimaryDark">
            <TextView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:background="@color/btn_color"
                    android:layout_alignParentBottom="true"></TextView>

        </RelativeLayout>

        <RelativeLayout
                android:id="@+id/rl_home"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_50sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginLeft="@dimen/_5sdp"
                android:visibility="gone"
                android:layout_marginRight="@dimen/_5sdp">
            <ImageView
                    android:id="@+id/img_home"
                    android:layout_width="@dimen/_16sdp"
                    android:layout_height="@dimen/_17sdp"
                    android:background="@drawable/home"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:layout_marginTop="@dimen/_9sdp"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/_5sdp"
                   >

            </ImageView>
            <TextView
                    android:id="@+id/tv_home"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:textSize="@dimen/_10sdp"
                    android:layout_toRightOf="@+id/img_home"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_7sdp"
                    android:text="Home"
                    android:textColor="@color/black"/>

            <TextView
                    android:id="@+id/tv_home_desc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:textSize="@dimen/_9sdp"
                    android:layout_toRightOf="@+id/img_home"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_3sdp"
                    android:layout_below="@+id/tv_home"
                    android:text="Check all our recent designs and popular ones available in our store"
                  />


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_shop"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_50sdp"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginLeft="@dimen/_5sdp"
            android:layout_marginRight="@dimen/_5sdp">
        <ImageView
                android:id="@+id/img_shop"
                android:layout_width="@dimen/_16sdp"
                android:layout_height="@dimen/_17sdp"
                android:background="@drawable/shopbyevent"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_centerVertical="true"
                android:layout_marginTop="@dimen/_9sdp"
                android:layout_marginRight="@dimen/_5sdp"
                >

        </ImageView>
        <TextView
                android:id="@+id/tv_shop"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:textSize="@dimen/_10sdp"
                android:layout_toRightOf="@+id/img_shop"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_marginTop="@dimen/_7sdp"
                android:text="Shop By Event"
                android:textColor="@color/black"/>

        <TextView
                android:id="@+id/tv_shop_desc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:textSize="@dimen/_9sdp"
                android:layout_toRightOf="@+id/img_shop"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_marginTop="@dimen/_3sdp"
                android:layout_below="@+id/tv_shop"
                android:text="Select your event and check all our designs event wise"
                />


    </RelativeLayout>
        <RelativeLayout
                android:id="@+id/rl_todaydeal"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_50sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginLeft="@dimen/_5sdp"
                android:visibility="gone"
                android:layout_marginRight="@dimen/_5sdp">
            <ImageView
                    android:id="@+id/img_todaydeal"
                    android:layout_width="@dimen/_16sdp"
                    android:layout_height="@dimen/_17sdp"
                    android:background="@drawable/todaydeal"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:layout_marginTop="@dimen/_9sdp"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/_5sdp"
                    >

            </ImageView>
            <TextView
                    android:id="@+id/tv_todaydeal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:textSize="@dimen/_10sdp"
                    android:layout_toRightOf="@+id/img_todaydeal"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_7sdp"

                    android:text="Today's Deal"
                    android:textColor="@color/black"/>

            <TextView
                    android:id="@+id/tv_dealdesc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:textSize="@dimen/_9sdp"
                    android:layout_toRightOf="@+id/img_todaydeal"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_3sdp"
                    android:layout_below="@+id/tv_todaydeal"
                    android:text="Get access of all our coupons and deals for today"
                    />


        </RelativeLayout>
        <RelativeLayout
